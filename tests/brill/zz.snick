proc f (ref float i)

    i := i / (0 + i) * 8;
end

proc main ()
    int x;
    int y;
    float m;
    float n;

    x := (3 + 3 + 8) / 4;
    y := -5 + x;
    m := y;
    while x > 1 do
        if m > 0 then
            write m;
            write n;
            if x / 0 < m then
                f(n);
            fi
        else
            f(m);
        fi
    od
end
