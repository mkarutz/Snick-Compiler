proc main ()
    int n;
    int m;

    write ">";
    read n;
    fib(n, m);
    write m;
    write "\n";
end

proc fib (val int n, ref int out)
    int f1;
    int f2;

    if n <= 1 then
        out := n;
    else
        fib(n - 2, f1);
        fib(n - 1, f2);
        out := f1 + f2;
    fi
end

