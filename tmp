call main
halt
main:
push_stack_frame 1
int_const r0, 4
load r1, 0
call p
load r0, 0
call_builtin print_int
string_const r0, "\n"
call_builtin print_string
pop_stack_frame 1
return
p:
push_stack_frame 4
store 1, r1
store 0, r0
int_const r0, 2
load r1, 0
mul_int r0, r0, r1
store 1, r0
pop_stack_frame 4
return
